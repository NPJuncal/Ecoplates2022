# T1T ####

T1T.f.c[,"AWCD"]<-NA
T1T.f.c[,"AWCD.SE"]<-NA
T1T.f.c[,"H"]<-NA
T1T.f.c[,"H.SE"]<-NA


for (i in 1:nrow(T1T.f.c)) {
  
  vec<-as.numeric(T1T.f.c[i,c(8:38)])
  A1<-T1T.f.c[i,7]
  T1T.f.c[i,"AWCD"]<- (sum(vec-as.numeric(A1))/31)
  T1T.f.c[i,"H"]<-diversity(as.numeric(T1T.f.c[i,c(8:38)]), "shannon")}

Rep1<-subset(T1T.f,T1T.f$Depth == 4 | T1T.f$Depth == 42)
Rep1[,"AWCD"]<-NA
Rep1[,"H"]<-NA

for (i in 1:nrow(Rep1)) {
  
  vec<-as.numeric(Rep1[i,c(8:38)])
  A1<-Rep1[i,7]
  Rep1[i,"AWCD"]<- (sum(vec-A1)/31)
  Rep1[i,"H"]<-diversity(Rep1[i,c(8:38)], "shannon")}


HAL_Ana_4<-subset(Rep1,Rep1$Station == "HAL" & Rep1$Depth == 4 & Rep1$Met == "ANA")
T1T.f.c[which(T1T.f.c$Station == "HAL" & T1T.f.c$Depth == 4 & T1T.f.c$Met == "ANA"),"AWCD.SE"]<-sd(HAL_Ana_4$AWCD)/nrow(HAL_Ana_4)
