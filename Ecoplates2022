BOL_Aer_42<-subset(Rep2,Rep2$Station == "BOL" & Rep2$Depth == 42 & Rep2$Met == "AER")
T2T.f.c[which(T2T.f.c$Station == "BOL" & T2T.f.c$Depth == 42 & T2T.f.c$Met == "AER"),"AWCD.SE"]<-sd(BOL_Aer_42$AWCD)/nrow(BOL_Aer_42)
T2T.f.c[which(T2T.f.c$Station == "BOL" & T2T.f.c$Depth == 42 & T2T.f.c$Met == "AER"),"H.SE"]<-sd(BOL_Aer_42$H)/nrow(BOL_Aer_42)


BB_Ana_4<-subset(Rep2,Rep2$Station == "BB" & Rep2$Depth == 4 & Rep2$Met == "ANA")
T2T.f.c[which(T2T.f.c$Station == "BB" & T2T.f.c$Depth == 4 & T2T.f.c$Met == "ANA"),"AWCD.SE"]<-sd(BB_Ana_4$AWCD)/nrow(BB_Ana_4)
T2T.f.c[which(T2T.f.c$Station == "BB" & T2T.f.c$Depth == 4 & T2T.f.c$Met == "ANA"),"H.SE"]<-sd(BB_Ana_4$H)/nrow(BB_Ana_4)
BB_Aer_4<-subset(Rep2,Rep2$Station == "BB" & Rep2$Depth == 4 & Rep2$Met == "AER")
T2T.f.c[which(T2T.f.c$Station == "BB" & T2T.f.c$Depth == 4 & T2T.f.c$Met == "AER"),"AWCD.SE"]<-sd(BB_Aer_4$AWCD)/nrow(BB_Aer_4)
T2T.f.c[which(T2T.f.c$Station == "BB" & T2T.f.c$Depth == 4 & T2T.f.c$Met == "AER"),"H.SE"]<-sd(BB_Aer_4$H)/nrow(BB_Aer_4)
BB_Ana_42<-subset(Rep2,Rep2$Station == "BB" & Rep2$Depth == 42 & Rep2$Met == "ANA")
T2T.f.c[which(T2T.f.c$Station == "BB" & T2T.f.c$Depth == 42 & T2T.f.c$Met == "ANA"),"AWCD.SE"]<-sd(BB_Ana_42$AWCD)/nrow(BB_Ana_42)
T2T.f.c[which(T2T.f.c$Station == "BB" & T2T.f.c$Depth == 42 & T2T.f.c$Met == "ANA"),"H.SE"]<-sd(BB_Ana_42$H)/nrow(BB_Ana_42)
BB_Aer_42<-subset(Rep2,Rep2$Station == "BB" & Rep2$Depth == 42 & Rep2$Met == "AER")
T2T.f.c[which(T2T.f.c$Station == "BB" & T2T.f.c$Depth == 42 & T2T.f.c$Met == "AER"),"AWCD.SE"]<-sd(BB_Aer_42$AWCD)/nrow(BB_Aer_42)
T2T.f.c[which(T2T.f.c$Station == "BB" & T2T.f.c$Depth == 42 & T2T.f.c$Met == "AER"),"H.SE"]<-sd(BB_Aer_42$H)/nrow(BB_Aer_42)


J_Ana_4<-subset(Rep2,Rep2$Station == "J" & Rep2$Depth == 4 & Rep2$Met == "ANA")
T2T.f.c[which(T2T.f.c$Station == "J" & T2T.f.c$Depth == 4 & T2T.f.c$Met == "ANA"),"AWCD.SE"]<-sd(J_Ana_4$AWCD)/nrow(J_Ana_4)
T2T.f.c[which(T2T.f.c$Station == "J" & T2T.f.c$Depth == 4 & T2T.f.c$Met == "ANA"),"H.SE"]<-sd(J_Ana_4$H)/nrow(J_Ana_4)
J_Aer_4<-subset(Rep2,Rep2$Station == "J" & Rep2$Depth == 4 & Rep2$Met == "AER")
T2T.f.c[which(T2T.f.c$Station == "J" & T2T.f.c$Depth == 4 & T2T.f.c$Met == "AER"),"AWCD.SE"]<-sd(J_Aer_4$AWCD)/nrow(J_Aer_4)
T2T.f.c[which(T2T.f.c$Station == "J" & T2T.f.c$Depth == 4 & T2T.f.c$Met == "AER"),"H.SE"]<-sd(J_Aer_4$H)/nrow(J_Aer_4)
J_Ana_42<-subset(Rep2,Rep2$Station == "J" & Rep2$Depth == 42 & Rep2$Met == "ANA")
T2T.f.c[which(T2T.f.c$Station == "J" & T2T.f.c$Depth == 42 & T2T.f.c$Met == "ANA"),"AWCD.SE"]<-sd(J_Ana_42$AWCD)/nrow(J_Ana_42)
T2T.f.c[which(T2T.f.c$Station == "J" & T2T.f.c$Depth == 42 & T2T.f.c$Met == "ANA"),"H.SE"]<-sd(J_Ana_42$H)/nrow(J_Ana_42)
J_Aer_42<-subset(Rep2,Rep2$Station == "J" & Rep2$Depth == 42 & Rep2$Met == "AER")
T2T.f.c[which(T2T.f.c$Station == "J" & T2T.f.c$Depth == 42 & T2T.f.c$Met == "AER"),"AWCD.SE"]<-sd(J_Aer_42$AWCD)/nrow(J_Aer_42)
T2T.f.c[which(T2T.f.c$Station == "J" & T2T.f.c$Depth == 42 & T2T.f.c$Met == "AER"),"H.SE"]<-sd(J_Aer_42$H)/nrow(J_Aer_42)


# T3T ####

T3T.f.c[,"AWCD"]<-NA
T3T.f.c[,"AWCD.SE"]<-NA
T3T.f.c[,"H"]<-NA
T3T.f.c[,"H.SE"]<-NA


for (i in 1:nrow(T3T.f.c)) {
  
  vec<-as.numeric(T3T.f.c[i,c(8:38)])
  A1<-T3T.f.c[i,7]
  T3T.f.c[i,"AWCD"]<- (sum(vec-as.numeric(A1))/31)
  T3T.f.c[i,"H"]<-diversity(as.numeric(T3T.f.c[i,c(8:38)]), "shannon")}

Rep3<-subset(T3T.f,T3T.f$Depth == 4 | T3T.f$Depth == 42)
Rep3[,"AWCD"]<-NA
Rep3[,"H"]<-NA

for (i in 1:nrow(Rep3)) {
  
  vec<-as.numeric(Rep3[i,c(8:38)])
  A1<-Rep3[i,7]
  Rep3[i,"AWCD"]<- (sum(vec-A1)/31)
  Rep3[i,"H"]<-diversity(Rep3[i,c(8:38)], "shannon")}


HAL_Ana_4<-subset(Rep3,Rep3$Station == "HAL" & Rep3$Depth == 4 & Rep3$Met == "ANA")
T3T.f.c[which(T3T.f.c$Station == "HAL" & T3T.f.c$Depth == 4 & T3T.f.c$Met == "ANA"),"AWCD.SE"]<-sd(HAL_Ana_4$AWCD)/nrow(HAL_Ana_4)
T3T.f.c[which(T3T.f.c$Station == "HAL" & T3T.f.c$Depth == 4 & T3T.f.c$Met == "ANA"),"H.SE"]<-sd(HAL_Ana_4$H)/nrow(HAL_Ana_4)
HAL_Aer_4<-subset(Rep3,Rep3$Station == "HAL" & Rep3$Depth == 4 & Rep3$Met == "AER")
T3T.f.c[which(T3T.f.c$Station == "HAL" & T3T.f.c$Depth == 4 & T3T.f.c$Met == "AER"),"AWCD.SE"]<-sd(HAL_Aer_4$AWCD)/nrow(HAL_Aer_4)
T3T.f.c[which(T3T.f.c$Station == "HAL" & T3T.f.c$Depth == 4 & T3T.f.c$Met == "AER"),"H.SE"]<-sd(HAL_Aer_4$H)/nrow(HAL_Aer_4)
HAL_Ana_42<-subset(Rep3,Rep3$Station == "HAL" & Rep3$Depth == 42 & Rep3$Met == "ANA")
T3T.f.c[which(T3T.f.c$Station == "HAL" & T3T.f.c$Depth == 42 & T3T.f.c$Met == "ANA"),"AWCD.SE"]<-sd(HAL_Ana_42$AWCD)/nrow(HAL_Ana_42)
T3T.f.c[which(T3T.f.c$Station == "HAL" & T3T.f.c$Depth == 42 & T3T.f.c$Met == "ANA"),"H.SE"]<-sd(HAL_Ana_42$H)/nrow(HAL_Ana_42)
HAL_Aer_42<-subset(Rep3,Rep3$Station == "HAL" & Rep3$Depth == 42 & Rep3$Met == "AER")
T3T.f.c[which(T3T.f.c$Station == "HAL" & T3T.f.c$Depth == 42 & T3T.f.c$Met == "AER"),"AWCD.SE"]<-sd(HAL_Aer_42$AWCD)/nrow(HAL_Aer_42)
T3T.f.c[which(T3T.f.c$Station == "HAL" & T3T.f.c$Depth == 42 & T3T.f.c$Met == "AER"),"H.SE"]<-sd(HAL_Aer_42$H)/nrow(HAL_Aer_42)


BOL_Ana_4<-subset(Rep3,Rep3$Station == "BOL" & Rep3$Depth == 4 & Rep3$Met == "ANA")
T3T.f.c[which(T3T.f.c$Station == "BOL" & T3T.f.c$Depth == 4 & T3T.f.c$Met == "ANA"),"AWCD.SE"]<-sd(BOL_Ana_4$AWCD)/nrow(BOL_Ana_4)
T3T.f.c[which(T3T.f.c$Station == "BOL" & T3T.f.c$Depth == 4 & T3T.f.c$Met == "ANA"),"H.SE"]<-sd(BOL_Ana_4$H)/nrow(BOL_Ana_4)
BOL_Aer_4<-subset(Rep3,Rep3$Station == "BOL" & Rep3$Depth == 4 & Rep3$Met == "AER")
T3T.f.c[which(T3T.f.c$Station == "BOL" & T3T.f.c$Depth == 4 & T3T.f.c$Met == "AER"),"AWCD.SE"]<-sd(BOL_Aer_4$AWCD)/nrow(BOL_Aer_4)
T3T.f.c[which(T3T.f.c$Station == "BOL" & T3T.f.c$Depth == 4 & T3T.f.c$Met == "AER"),"H.SE"]<-sd(BOL_Aer_4$H)/nrow(BOL_Aer_4)
BOL_Ana_42<-subset(Rep3,Rep3$Station == "BOL" & Rep3$Depth == 42 & Rep3$Met == "ANA")
T3T.f.c[which(T3T.f.c$Station == "BOL" & T3T.f.c$Depth == 42 & T3T.f.c$Met == "ANA"),"AWCD.SE"]<-sd(BOL_Ana_42$AWCD)/nrow(BOL_Ana_42)
T3T.f.c[which(T3T.f.c$Station == "BOL" & T3T.f.c$Depth == 42 & T3T.f.c$Met == "ANA"),"H.SE"]<-sd(BOL_Ana_42$H)/nrow(BOL_Ana_42)
BOL_Aer_42<-subset(Rep3,Rep3$Station == "BOL" & Rep3$Depth == 42 & Rep3$Met == "AER")
T3T.f.c[which(T3T.f.c$Station == "BOL" & T3T.f.c$Depth == 42 & T3T.f.c$Met == "AER"),"AWCD.SE"]<-sd(BOL_Aer_42$AWCD)/nrow(BOL_Aer_42)
T3T.f.c[which(T3T.f.c$Station == "BOL" & T3T.f.c$Depth == 42 & T3T.f.c$Met == "AER"),"H.SE"]<-sd(BOL_Aer_42$H)/nrow(BOL_Aer_42)


BB_Ana_4<-subset(Rep3,Rep3$Station == "BB" & Rep3$Depth == 4 & Rep3$Met == "ANA")
T3T.f.c[which(T3T.f.c$Station == "BB" & T3T.f.c$Depth == 4 & T3T.f.c$Met == "ANA"),"AWCD.SE"]<-sd(BB_Ana_4$AWCD)/nrow(BB_Ana_4)
T3T.f.c[which(T3T.f.c$Station == "BB" & T3T.f.c$Depth == 4 & T3T.f.c$Met == "ANA"),"H.SE"]<-sd(BB_Ana_4$H)/nrow(BB_Ana_4)
BB_Aer_4<-subset(Rep3,Rep3$Station == "BB" & Rep3$Depth == 4 & Rep3$Met == "AER")
T3T.f.c[which(T3T.f.c$Station == "BB" & T3T.f.c$Depth == 4 & T3T.f.c$Met == "AER"),"AWCD.SE"]<-sd(BB_Aer_4$AWCD)/nrow(BB_Aer_4)
T3T.f.c[which(T3T.f.c$Station == "BB" & T3T.f.c$Depth == 4 & T3T.f.c$Met == "AER"),"H.SE"]<-sd(BB_Aer_4$H)/nrow(BB_Aer_4)
BB_Ana_42<-subset(Rep3,Rep3$Station == "BB" & Rep3$Depth == 42 & Rep3$Met == "ANA")
T3T.f.c[which(T3T.f.c$Station == "BB" & T3T.f.c$Depth == 42 & T3T.f.c$Met == "ANA"),"AWCD.SE"]<-sd(BB_Ana_42$AWCD)/nrow(BB_Ana_42)
T3T.f.c[which(T3T.f.c$Station == "BB" & T3T.f.c$Depth == 42 & T3T.f.c$Met == "ANA"),"H.SE"]<-sd(BB_Ana_42$H)/nrow(BB_Ana_42)
BB_Aer_42<-subset(Rep3,Rep3$Station == "BB" & Rep3$Depth == 42 & Rep3$Met == "AER")
T3T.f.c[which(T3T.f.c$Station == "BB" & T3T.f.c$Depth == 42 & T3T.f.c$Met == "AER"),"AWCD.SE"]<-sd(BB_Aer_42$AWCD)/nrow(BB_Aer_42)
T3T.f.c[which(T3T.f.c$Station == "BB" & T3T.f.c$Depth == 42 & T3T.f.c$Met == "AER"),"H.SE"]<-sd(BB_Aer_42$H)/nrow(BB_Aer_42)


J_Ana_4<-subset(Rep3,Rep3$Station == "J" & Rep3$Depth == 4 & Rep3$Met == "ANA")
T3T.f.c[which(T3T.f.c$Station == "J" & T3T.f.c$Depth == 4 & T3T.f.c$Met == "ANA"),"AWCD.SE"]<-sd(J_Ana_4$AWCD)/nrow(J_Ana_4)
T3T.f.c[which(T3T.f.c$Station == "J" & T3T.f.c$Depth == 4 & T3T.f.c$Met == "ANA"),"H.SE"]<-sd(J_Ana_4$H)/nrow(J_Ana_4)
J_Aer_4<-subset(Rep3,Rep3$Station == "J" & Rep3$Depth == 4 & Rep3$Met == "AER")
T3T.f.c[which(T3T.f.c$Station == "J" & T3T.f.c$Depth == 4 & T3T.f.c$Met == "AER"),"AWCD.SE"]<-sd(J_Aer_4$AWCD)/nrow(J_Aer_4)
T3T.f.c[which(T3T.f.c$Station == "J" & T3T.f.c$Depth == 4 & T3T.f.c$Met == "AER"),"H.SE"]<-sd(J_Aer_4$H)/nrow(J_Aer_4)
J_Ana_42<-subset(Rep3,Rep3$Station == "J" & Rep3$Depth == 42 & Rep3$Met == "ANA")
T3T.f.c[which(T3T.f.c$Station == "J" & T3T.f.c$Depth == 42 & T3T.f.c$Met == "ANA"),"AWCD.SE"]<-sd(J_Ana_42$AWCD)/nrow(J_Ana_42)
T3T.f.c[which(T3T.f.c$Station == "J" & T3T.f.c$Depth == 42 & T3T.f.c$Met == "ANA"),"H.SE"]<-sd(J_Ana_42$H)/nrow(J_Ana_42)
J_Aer_42<-subset(Rep3,Rep3$Station == "J" & Rep3$Depth == 42 & Rep3$Met == "AER")
T3T.f.c[which(T3T.f.c$Station == "J" & T3T.f.c$Depth == 42 & T3T.f.c$Met == "AER"),"AWCD.SE"]<-sd(J_Aer_42$AWCD)/nrow(J_Aer_42)
T3T.f.c[which(T3T.f.c$Station == "J" & T3T.f.c$Depth == 42 & T3T.f.c$Met == "AER"),"H.SE"]<-sd(J_Aer_42$H)/nrow(J_Aer_42)



############################
### Temporal differences ###
############################

# built data matrix of average and sd of AWCD and H per station over time ####
TEMP<-cbind(T1T.f.c[,c(1:5,39,41)],T2T.f.c[,c(39,41)],T3T.f.c[,c(39,41)],B[,c(39,41)])
colnames(TEMP)<-c("Sample","Station","Core","Depth","Met","AWCD.T1","H.T1","AWCD.T2","H.T2","AWCD.T3","H.T3","AWCD.T4","H.T4")


Av.TEMP<- data.frame(Station=character(), 
                     Metabolism=character(),
                     AWCD.T1=numeric(),
                     H.T1=numeric(),
                     AWCD.T2=numeric(),
                     H.T2=numeric(),
                     AWCD.T3=numeric(),
                     H.T3=numeric(),
                     AWCD.T4=numeric(),
                     H.T4=numeric(),
                     AWCD.T1.sd=numeric(),
                     H.T1.sd=numeric(),
                     AWCD.T2.sd=numeric(),
                     H.T2.sd=numeric(),
                     AWCD.T3.sd=numeric(),
                     H.T3.sd=numeric(),
                     AWCD.T4.sd=numeric(),
                     H.T4.sd=numeric(),
                     Label=character())

Stat<-subset(TEMP,TEMP$Station == "ATR" & TEMP$Met == "AER")
Av.TEMP[1,]<- c(as.character(Stat[1,c(2,5)]), as.numeric(colMeans(Stat[,c(6:13)])),
                as.numeric(apply(Stat[,c(6:13)],2,sd)),NA)
Stat<-subset(TEMP,TEMP$Station == "ATR" & TEMP$Met == "ANA")
Av.TEMP[2,]<- c(as.character(Stat[1,c(2,5)]), as.numeric(colMeans(Stat[,c(6:13)])),
                as.numeric(apply(Stat[,c(6:13)],2,sd)),NA)
Stat<-subset(TEMP,TEMP$Station == "BOL" & TEMP$Met == "AER")
Av.TEMP[3,]<- c(as.character(Stat[1,c(2,5)]), as.numeric(colMeans(Stat[,c(6:13)])),
                as.numeric(apply(Stat[,c(6:13)],2,sd)),NA)
Stat<-subset(TEMP,TEMP$Station == "BOL" & TEMP$Met == "ANA")
Av.TEMP[4,]<- c(as.character(Stat[1,c(2,5)]), as.numeric(colMeans(Stat[,c(6:13)])),
                as.numeric(apply(Stat[,c(6:13)],2,sd)),NA)
Stat<-subset(TEMP,TEMP$Station == "BB" & TEMP$Met == "AER")
Av.TEMP[5,]<- c(as.character(Stat[1,c(2,5)]), as.numeric(colMeans(Stat[,c(6:13)])),
                as.numeric(apply(Stat[,c(6:13)],2,sd)),NA)
Stat<-subset(TEMP,TEMP$Station == "BB" & TEMP$Met == "ANA")
Av.TEMP[6,]<- c(as.character(Stat[1,c(2,5)]), as.numeric(colMeans(Stat[,c(6:13)])),
                as.numeric(apply(Stat[,c(6:13)],2,sd)),NA)
Stat<-subset(TEMP,TEMP$Station == "J" & TEMP$Met == "AER")
Av.TEMP[7,]<- c(as.character(Stat[1,c(2,5)]), as.numeric(colMeans(Stat[,c(6:13)])),
                as.numeric(apply(Stat[,c(6:13)],2,sd)),NA)
Stat<-subset(TEMP,TEMP$Station == "J" & TEMP$Met == "ANA")
Av.TEMP[8,]<- c(as.character(Stat[1,c(2,5)]), as.numeric(colMeans(na.omit(Stat[,c(6:13)]))),
                as.numeric(apply(Stat[,c(6:13)],2,sd,na.rm=TRUE)),NA)

Av.TEMP<-Av.TEMP %>%
  unite(Label, Station, Metabolism, sep="_", remove = FALSE)

Av.TEMP[,c(4:19)] <- sapply(Av.TEMP[,c(4:19)],as.numeric)

write.csv(Av.TEMP,"Av.TEMP.csv",sep=";", dec=",")

# times comparison 

p1<-ggplot(TEMP, aes(AWCD.T1,AWCD.T4))+
  geom_point(aes(color=Station,shape=Met))

p2<-ggplot(TEMP, aes(H.T1,H.T4))+
  geom_point(aes(color=Station,shape=Met))

g<-gridExtra::grid.arrange(p1,p2, ncol=2)
ggsave("Comp.T1-T4.jpg", g, width = 19, height =7.5, units = "cm")


########################################################
### Significant differences between metabolisms (T4) ###
########################################################

C<- B[-c(20,21,42),] #data.frame with only samples where we have data for both AER and ANA 

#AWCD

AWCD.AER<-C$AWCD[C$Met == "AER"]
AWCD.ANA<-C$AWCD[C$Met == "ANA"]

shapiro.test(AWCD.AER) ### normality (>0.05 normal, <0.05 no normal) 

#F-test requires normality

var.test(AWCD ~ Met, data = C) #>0.05 no significant difference between the two variances

t.test(AWCD.AER, AWCD.ANA) # p <0.05 significant differences

#ggplot(C,aes(x=factor(Met), AWCD)) + 
#geom_boxplot(fill="grey",alpha=0.5)+
#geom_jitter(aes(color=Station))+
#scale_colour_manual(values = c("blue","red","orange","black"))


Bxp.AWCD<-ggplot(C,aes(x=factor(Met), AWCD)) + ylab("Average Well Color Development (AWCD)")+
  geom_boxplot(aes(x=factor(Met), AWCD),fill="grey",alpha=0.5)+
  geom_point(aes(color=Station),position=position_dodge(width=0.5))+
  scale_colour_manual(values = c("blue","red","orange","black"))+
  geom_errorbar(
    aes(ymin=AWCD-AWCD.SE, ymax=AWCD+AWCD.SE, color=Station),
    width = 0.1,
    #linetype = "dotted",
    position=position_dodge(width=0.5))+
  theme(axis.title.x=element_blank(),
        legend.position="none")


ggplot(C,aes(x=factor(Met), AWCD)) + ylab("Average Well Color Development (AWCD)")+
  geom_boxplot(aes(x=factor(Met), AWCD),fill="grey",alpha=0.5)+
  geom_point(aes(color=Station),position=position_dodge(width=0.5))+
  scale_colour_manual(values = c("blue","red","orange","black"))+
  geom_errorbar(
    aes(ymin=AWCD-AWCD.SE, ymax=AWCD+AWCD.SE, color=Station),
    width = 0.1,
    #linetype = "dotted",
    position=position_dodge(width=0.5))+
  facet_wrap(~Station, scale="fixed")

C$Depth<-as.numeric(C$Depth)

ggplot(C,aes(Depth,AWCD))+ ylab("Average Well Color Development (AWCD)")+
  geom_point(aes(color=Met))+
  coord_flip()+
  scale_x_reverse()+
  facet_wrap(~Station, scale="fixed")


####Shannon-Wiener Diversity

H.AER<-C$H[C$Met == "AER"]
H.ANA<-C$H[C$Met == "ANA"]

H.AER<-H.AER[-21]#delete outlier
H.ANA<-H.ANA[-21]#in the other met as well to compensate the groups

shapiro.test(H.ANA) ### normality (>0.05 normal, <0.05 no normal) 

#F-test requires normality

var.test(H ~ Met, data = C[-c(39,42),]) #>0.05 no significant difference between the two variances
#C[-c(39,42),] because of the outlier
t.test(H.AER, H.ANA)

#ggplot(C[-c(39,42),],aes(x=factor(Met), H)) + 
#geom_boxplot(fill="grey",alpha=0.5)+
#geom_jitter(aes(color=Station))+
#scale_colour_manual(values = c("blue","red","orange","black"))

#ggplot(C[-c(39,42),], aes(x=Station, y=H, fill=Met)) + 
#geom_boxplot() + geom_jitter()+
#facet_wrap(~Station, scale="free")

Bxp.H<-ggplot(C,aes(x=factor(Met), H)) + ylab("Shannon-Wiener diversity index (H')")+
  geom_boxplot(aes(x=factor(Met), H),fill="grey",alpha=0.5)+
  geom_point(aes(color=Station),position=position_dodge(width=0.5))+
  scale_colour_manual(values = c("blue","red","orange","black"))+
  geom_errorbar(
    aes(ymin=H-H.SE, ymax=H+H.SE, color=Station),
    width = 0.1,
    #linetype = "dotted",
    position=position_dodge(width=0.5))+
  theme(axis.title.x=element_blank())

ggplot(C,aes(x=factor(Met), H)) + ylab("Shannon-Wiener diversity index (H')")+
  geom_boxplot(aes(x=factor(Met), H),fill="grey",alpha=0.5)+
  geom_point(aes(color=Station),position=position_dodge(width=0.5))+
  scale_colour_manual(values = c("blue","red","orange","black"))+
  geom_errorbar(
    aes(ymin=H-H.SE, ymax=H+H.SE, color=Station),
    width = 0.1,
    #linetype = "dotted",
    position=position_dodge(width=0.5))+
  facet_wrap(~Station, scale="fixed")

#Aer.vs.ana<-grid.arrange(Bxp.AWCD,Bxp.H,nrow=1, top="Aerobic (AER) vs Anaerobic (ANA) incubations")

Aer.vs.ana<-plot_grid(Bxp.AWCD,Bxp.H, align = "h", nrow = 1, rel_widths = c(5/6, 1),labels=c('A', 'B'))

ggsave("Aer.vs.ana.jpg",Aer.vs.ana, units="cm", width = 20, height = 10)


############################
### Compound consumption ###
############################

AER.B<-filter(B,B$Met == "AER")
AER.B$Depth[AER.B$Depth == "4"] <- "04"
AER.B<-AER.B %>% mutate(Sample=paste(AER.B$Station,AER.B$Depth))

ANA.B<-filter(B,B$Met == "ANA")
ANA.B$Depth[ANA.B$Depth == "4"] <- "04"
ANA.B<-ANA.B %>% mutate(Sample=paste(ANA.B$Station,ANA.B$Depth))

write.csv(B,"FinalData.csv",sep=";", dec=",")

### Average compound consumption per station

MeansSt<-data.frame(ID=character(),
                    MetID=character())

MeansSt[1,1]<-"ATR"
MeansSt[1,2]<-"AER"
AER.B_ATR<-filter(AER.B,AER.B$Station == "ATR")
AER.B_ATR[,c(8:38)]<-apply(AER.B_ATR[,c(8:38)],2,as.numeric)
MeansSt[1,c(3:33)]<-apply(AER.B_ATR[,c(8:38)],2,mean)

MeansSt[2,1]<-"ATR"
MeansSt[2,2]<-"ANA"
ANA.B_ATR<-filter(ANA.B,ANA.B$Station == "ATR")
ANA.B_ATR[,c(8:38)]<-apply(ANA.B_ATR[,c(8:38)],2,as.numeric)
MeansSt[2,c(3:33)]<-apply(ANA.B_ATR[,c(8:38)],2,mean)

MeansSt[3,1]<-"BOL"
MeansSt[3,2]<-"AER"
AER.B_BOL<-filter(AER.B,AER.B$Station == "BOL")
AER.B_BOL[,c(8:38)]<-apply(AER.B_BOL[,c(8:38)],2,as.numeric)
MeansSt[3,c(3:33)]<-apply(AER.B_BOL[,c(8:38)],2,mean)

MeansSt[4,1]<-"BOL"
MeansSt[4,2]<-"ANA"
ANA.B_BOL<-filter(ANA.B,ANA.B$Station == "BOL")
ANA.B_BOL[,c(8:38)]<-apply(ANA.B_BOL[,c(8:38)],2,as.numeric)
MeansSt[4,c(3:33)]<-apply(ANA.B_BOL[,c(8:38)],2,mean)

MeansSt[5,1]<-"J"
MeansSt[5,2]<-"AER"
AER.B_J<-filter(AER.B,AER.B$Station == "J")
AER.B_J[,c(8:38)]<-apply(AER.B_J[,c(8:38)],2,as.numeric)
MeansSt[5,c(3:33)]<-apply(AER.B_J[,c(8:38)],2,mean)

MeansSt[6,1]<-"J"
MeansSt[6,2]<-"ANA"
ANA.B_J<-filter(ANA.B,ANA.B$Station == "J")
ANA.B_J[,c(8:38)]<-apply(ANA.B_J[,c(8:38)],2,as.numeric)
MeansSt[6,c(3:33)]<-apply(ANA.B_J[,c(8:38)],2,mean)

MeansSt[7,1]<-"BB"
MeansSt[7,2]<-"AER"
AER.B_BB<-filter(AER.B,AER.B$Station == "BB")
AER.B_BB[,c(8:38)]<-apply(AER.B_BB[,c(8:38)],2,as.numeric)
MeansSt[7,c(3:33)]<-apply(AER.B_BB[,c(8:38)],2,mean)

MeansSt[8,1]<-"BB"
MeansSt[8,2]<-"ANA"
ANA.B_BB<-filter(ANA.B,ANA.B$Station == "BB")
ANA.B_BB[,c(8:38)]<-apply(ANA.B_BB[,c(8:38)],2,as.numeric)
MeansSt[8,c(3:33)]<-apply(ANA.B_BB[,c(8:38)],2,mean)

CS<-colnames(B[,c(8:38)])
colnames(MeansSt)<-c("Station","Met",CS)

MeansSt<-MeansSt %>% mutate(Sample=paste(MeansSt$Station,MeansSt$Met))
MeansStm <- melt(MeansSt[,c(3:34)], id.vars="Sample")
MeansStm[,'Guilt'] <- MeansStm[,2]
MeansStm$Guilt<-as.character(MeansStm$Guilt)

MeansStm$Guilt[MeansStm$Guilt == "B1"|MeansStm$Guilt == "G2"|MeansStm$Guilt == "H2"] <- "Miscellaneous "
MeansStm$Guilt[MeansStm$Guilt == "C1"|MeansStm$Guilt == "D1"|MeansStm$Guilt == "F1"|MeansStm$Guilt == "E1"] <- "Polymers"
MeansStm$Guilt[MeansStm$Guilt == "G1"|MeansStm$Guilt =="H1"|MeansStm$Guilt == "A2"| MeansStm$Guilt =="B2"| MeansStm$Guilt =="C2"| MeansStm$Guilt =="D2"| MeansStm$Guilt =="E2"] <- "Carbohydrates"
MeansStm$Guilt[MeansStm$Guilt == "F2"|MeansStm$Guilt =="A3"|MeansStm$Guilt == "B3"| MeansStm$Guilt =="C3"| MeansStm$Guilt =="D3"| MeansStm$Guilt =="E3"| MeansStm$Guilt =="F3"| MeansStm$Guilt =="G3"| MeansStm$Guilt =="H3"] <- "Carboxylic acids"
MeansStm$Guilt[MeansStm$Guilt == "A4"|MeansStm$Guilt =="B4"|MeansStm$Guilt == "C4"| MeansStm$Guilt =="D4"| MeansStm$Guilt =="E4"| MeansStm$Guilt =="F4"] <- "Amino acids"
MeansStm$Guilt[MeansStm$Guilt == "G4"|MeansStm$Guilt == "H4"] <- "Amines"


CsC<-ggplot(MeansStm, aes(value,Sample))+ xlab("Average Color Development")+ ylab("Station & Metabolism")+
  geom_point()+
  #geom_rect(aes(xmin=(mean(B$A1)-sd(B$A1)), xmax=(mean(B$A1)+sd(B$A1)), ymin=0, ymax=Inf),fill="grey",alpha=0.02)+
  geom_vline(xintercept = mean(as.numeric(B$A1)))+
  geom_vline(xintercept = mean(as.numeric(B$A1))-sd(as.numeric(B$A1)),linetype = "dashed")+
  geom_vline(xintercept = mean(as.numeric(B$A1))+sd(as.numeric(B$A1)),linetype = "dashed")+
  facet_wrap(~Guilt, scale="fixed", ncol = 2)

ggsave("Carbon sources comp.jpg",CsC, units="cm", width = 10, height = 20)



##################################
### Raw results representation ###
##################################

#Load Data again to use not transformed data!

Data<-read.csv("Cores.csv", header=T, sep=";", dec=".")
Data<-as.data.frame(Data)

Data2<-subset(Data, Data$Core_ID == "ATR_C" | Data$Core_ID == "BOL_A" | Data$Core_ID == "BB_A" | Data$Core_ID == "J_C")

#OM and CaCO3 

# signifficative diferences

shapiro.test(Data$OM)### normality (>0.05 normal, <0.05 no normal) 
shapiro.test(log(Data$OM))

anova<-(aov(log(OM) ~ Station, Data)) # ANOVA
summary(anova)
tukey<-TukeyHSD(aov(log(OM) ~ Station, Data))
print(tukey)


## collecting the groups information for the boxplot
library(multcompView)
let<-multcompLetters4(anova,tukey)
print(let)

Tk<-group_by(Data,Station)%>%
  summarise(mean=mean(OM,,na.rm=T),quant=quantile(OM, probs=0.75,na.rm=T)) %>%
  arrange(desc(mean))

let<-as.data.frame.list(let$Station)
Tk$let<-let$Letters
print(Tk)


#CaCO3
shapiro.test(Data$CaCO3) ### normality (>0.05 normal, <0.05 no normal) 
shapiro.test(log(Data$CaCO3))

#summary(aov(CaCO3 ~ Station, Data)) # ANOVA
#TukeyHSD(aov(CaCO3 ~ Station, Data))


pair<-pairwise.wilcox.test(Data$CaCO3, Data$Station,
                           p.adjust.method = "BH")
print(pair)

#manually adding the letter for the boxplots
Tk2<-group_by(Data,Station)%>%
  summarise(mean=mean(CaCO3,,na.rm=T),quant=quantile(OM, probs=0.75,na.rm=T)) %>%
  arrange(desc(mean))
Tk2$let<-c("b","b","b","a")

# plots

P1<-ggplot(Data2,aes(OM,Depth))+
  ggtitle("A") +
  xlab("%")+
  geom_point(aes(OM,Depth, shape="Organic matter"))+
  geom_point(aes(CaCO3,Depth, shape="Carbonates"))+
  scale_y_reverse()+
  facet_grid( ~ Station, scale ='fixed')+
  theme(plot.title = element_text(hjust = 0))

P2<-ggplot(Data,aes(x=factor(Station), OM)) + ylab("Organic Matter % (LOI 450¬∫C)")+xlab("Station")+
  ggtitle("B") +
  geom_boxplot(aes(x=factor(Station), OM),fill="grey",alpha=0.5)+
  #geom_jitter()+
  geom_text(data=Tk, aes(label=let, x=Station, y=50), size=4)
#hjust=-1,

P3<-ggplot(Data,aes(x=factor(Station), CaCO3)) + labs(y = expression("CaCO"["3"] ~ "% (LOI 950¬∫C)"))+xlab("Station")+
  ggtitle("C") +
  geom_boxplot(aes(x=factor(Station), CaCO3),fill="grey",alpha=0.5)+
  #geom_jitter()+
  geom_text(data=Tk2, aes(label=let, x=Station, y=17), size=4)


OM_CaCO3<-grid.arrange(P1, P2, P3, nrow=2 ,ncol = 2, layout_matrix = cbind(c(1,2),c(1,3)))

ggsave("OM-CaCO3.jpg",OM_CaCO3, units="cm", width = 19, height = 15)

## grain size

DataGr <- melt(Data2[,c(2,5:12)], id.vars = c("Depth","Station"))
DataGr$variable <- factor(DataGr$variable,levels = c("Clay","Silt","F_Sand","M_Sand","C_Sand","VC_Sand","Gravel"))
DataGr$value<-DataGr$value*100

P1<-ggplot(DataGr, aes(fill=variable, y=value, x=Depth)) + ylab("Proportion of grain size particles")+
  coord_flip()+
  ggtitle("A") +
  scale_x_reverse()+
  scale_y_continuous(labels = function(x) {
    x[seq_along(x) %% 2 == 1] <- ""
    x
  })+
  facet_grid( ~ Station, scale ='fixed')+
  geom_bar(position="fill", stat="identity")+
  scale_fill_grey(start = 0, end = .9)+
  theme_bw()+
  theme(plot.title = element_text(hjust = 0))


P2<-ggplot(Data,aes(x=factor(Station), Clay)) + ylab("Clay (<0.063 mm) % ")+
  geom_boxplot(aes(x=factor(Station), Clay),fill="grey",alpha=0.5)+
  geom_jitter()+
  ggtitle("B") +
  theme(axis.text.x=element_blank(),
        axis.ticks.x=element_blank(),
        axis.title.x = element_blank()
        #axis.text.y=element_blank(),
        #axis.ticks.y=element_blank()
  )

P3<-ggplot(Data,aes(x=factor(Station), Silt)) + ylab("Silt (0.063-0.125 mm) %")+xlab("Station")+
  ggtitle("C") +
  geom_boxplot(aes(x=factor(Station), Silt),fill="grey",alpha=0.5)+
  geom_jitter()


GranDis<-grid.arrange(P1, P2, P3, ncol = 3, 
             layout_matrix = cbind(c(1,1), c(1,1),c(2,3)))

ggsave("GranDis.jpg",GranDis, units="cm", width = 19, height = 15)
