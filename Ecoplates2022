### load data without outlayers and generate final values per plate and compound###
###################################################################################

# T4T ####
File<-"FT4_cv.csv"

T4T<-read.csv(File, header=T, sep=";", dec=".")
T4T<-as.data.frame(T4T)

#% of eliminated values over the total

pc.dv<-((sum(is.na(T4T[-4,c(7:102)])))*100)/(nrow(T4T[-4,c(7:102)])*ncol(T4T[-4,c(7:102)]))
print(pc.dv)

# estimation of the mean value per plate and compound

FR<-T4T[,c(7:38)]#First replicate
SR<-T4T[,c(39:70)]#second replicate
TR<-T4T[,c(71:102)]#third replicate

all.dat <- list(dat1=FR, dat2=SR, dat3=TR)
T4T_m = aaply(laply(all.dat, as.matrix), c(2, 3), mean, na.rm=TRUE)

DF<-cbind(T4T[,c(1:6)],T4T_m)


#Subtract plates measured without lid

DF<-subset(DF, Tampa!="Nao")
write.csv(DF,"DF.csv",sep=";", dec=",")

# T1T ####

File<-"FT1_cv.csv"
T1T<-read.csv(File, header=T, sep=";", dec=".")
T1T<-as.data.frame(T1T)

#% of eliminated values over the total

pc.dv<-((sum(is.na(T1T[-4,c(7:102)])))*100)/(nrow(T1T[-4,c(7:102)])*ncol(T1T[-4,c(7:102)]))
print(pc.dv)

# estimation of the mean value per plate and compound

FR<-T1T[,c(7:38)]#First replicate
SR<-T1T[,c(39:70)]#second replicate
TR<-T1T[,c(71:102)]#third replicate

all.dat <- list(dat1=FR, dat2=SR, dat3=TR)
T1T_m = aaply(laply(all.dat, as.matrix), c(2, 3), mean, na.rm=TRUE)

T1T.f<-cbind(T1T[,c(1:6)],T1T_m)

#Substract plates measured without lid

T1T.f<-subset(T1T.f, Tampa!="Nao")
write.csv(T1T.f,"T1T.f.csv",sep=";", dec=",")

# T2T ####

File<-"FT2_cv.csv"
T2T<-read.csv(File, header=T, sep=";", dec=".")
T2T<-as.data.frame(T2T)

#% of eliminated values over the total

pc.dv<-((sum(is.na(T2T[-4,c(7:102)])))*100)/(nrow(T2T[-4,c(7:102)])*ncol(T2T[-4,c(7:102)]))
print(pc.dv)

# estimation of the mean value per plate and compound

FR<-T2T[,c(7:38)]#First replicate
SR<-T2T[,c(39:70)]#second replicate
TR<-T2T[,c(71:102)]#third replicate

all.dat <- list(dat1=FR, dat2=SR, dat3=TR)
T2T_m = aaply(laply(all.dat, as.matrix), c(2, 3), mean, na.rm=TRUE)

T2T.f<-cbind(T2T[,c(1:6)],T2T_m)

#Substract plates measured without lid

T2T.f<-subset(T2T.f, Tampa!="Nao")
write.csv(T2T.f,"T2T.f.csv",sep=";", dec=",")

# T3T ####

File<-"FT3_cv.csv"
T3T<-read.csv(File, header=T, sep=";", dec=".")
T3T<-as.data.frame(T3T)

#% of eliminated values over the total

pc.dv<-((sum(is.na(T3T[-4,c(7:102)])))*100)/(nrow(T3T[-4,c(7:102)])*ncol(T3T[-4,c(7:102)]))
print(pc.dv)

# estimation of the mean value per plate and compound

FR<-T3T[,c(7:38)]#First replicate
SR<-T3T[,c(39:70)]#second replicate
TR<-T3T[,c(71:102)]#third replicate

all.dat <- list(dat1=FR, dat2=SR, dat3=TR)
T3T_m = aaply(laply(all.dat, as.matrix), c(2, 3), mean, na.rm=TRUE)

