#############################
#load and clean data from T0#
#############################

File<-"T0.csv"

A<-read.csv(File, header=T, sep=";", dec=".")
T0<-as.data.frame(A)


# Load T4

File<-"T4.csv"

A<-read.csv(File, header=T, sep=";", dec=".")
T4<-as.data.frame(A)



#substract T0 to T4

FT4<-cbind(T4[,c(1:6)],(T4[,c(7:102)])-(T0[,c(7:102)]))

#some of the values for the control wells A0,A5,A9 and other wells were there was no consumption are negative
#measure method error? we check how negative the values

min(na.omit(FT4[,c(7:102)]))

write.csv(FT4,"FT4.csv",sep=";", dec=",")

#substitute negative values for 0
FT4[FT4 < 0] <- 0 

write.csv(FT4,"FT4_0.csv",sep=";", dec=",")

# estimation of the Coefficient of variation among plate replicates

FR<-FT4[,c(7:38)]#First replicate
SR<-FT4[,c(39:70)]#second replicate
TR<-FT4[,c(71:102)]#third replicate

all.dat <- list(dat1=FR, dat2=SR, dat3=TR)
FLR = aaply(laply(all.dat, as.matrix), c(2, 3), mean) # media de las tres replicas
FLR.sd = aaply(laply(all.dat, as.matrix), c(2, 3), sd) # estandar deviation of three replicates
FLR.cv<-FLR.sd/FLR # var coef of three replicates

count(as.data.frame(FLR.cv>0.3))


#There are some value in our plates that are very different from their replicates
#For example HAL C 40-42 ANA T0 (A1: 0.5, A5: 0.1, A9: 0.1)
#In this case we want to identify the deviated value and eliminate it
#From FLR.cv we are going to get a list of compounds and plates were cv is higher than 0.3

coords<-as.data.frame(which(FLR.cv > 0.3, arr.ind = TRUE))

CV<- data.frame(R1=numeric(), 
                R2=numeric(),
                R3=numeric())#dataframe values to check


for(i in 1:nrow(coords)) {
  
  CV[i,1]<-FR[coords[i,1],coords[i,2]]
  CV[i,2]<-SR[coords[i,1],coords[i,2]]
  CV[i,3]<-TR[coords[i,1],coords[i,2]]
  
}

CV<-cbind(coords,CV) # visual identification of outlayer


write.csv(CV,"CV.csv",sep=";", dec=",")


##########################
### Clean T1, T2 ad T3 ###
##########################

# Load Data

File<-"T1.csv"
A<-read.csv(File, header=T, sep=";", dec=".")
T1<-as.data.frame(A)

File<-"T2.csv"
A<-read.csv(File, header=T, sep=";", dec=".")
T2<-as.data.frame(A)

File<-"T3.csv"
A<-read.csv(File, header=T, sep=";", dec=".")
T3<-as.data.frame(A)

#substract T0 to Tx

FT1<-cbind(T1[,c(1:6)],(T1[,c(7:102)])-(T0[,c(7:102)]))
FT2<-cbind(T2[,c(1:6)],(T2[,c(7:102)])-(T0[,c(7:102)]))
FT3<-cbind(T3[,c(1:6)],(T3[,c(7:102)])-(T0[,c(7:102)]))


#some of the values for the control wells A0,A5,A9 and other wells were there was no consumption are negative
#measure method error? we check how negative the values

min(na.omit(FT1[,c(7:102)]))
min(na.omit(FT2[,c(7:102)]))
min(na.omit(FT3[,c(7:102)]))

#substitute negative values for 0
FT1[FT1 < 0] <- 0
FT2[FT2 < 0] <- 0
FT3[FT3 < 0] <- 0

write.csv(FT1,"FT1.csv",sep=";", dec=",")
write.csv(FT2,"FT2.csv",sep=";", dec=",")
write.csv(FT3,"FT3.csv",sep=";", dec=",")

# estimation of the Coefficient of variation among plate replicates

### FT1 ####
FR<-FT1[,c(7:38)]#First replicate
SR<-FT1[,c(39:70)]#second replicate
TR<-FT1[,c(71:102)]#third replicate

all.dat <- list(dat1=FR, dat2=SR, dat3=TR)
FLR.1 = aaply(laply(all.dat, as.matrix), c(2, 3), mean) # media de las tres replicas
FLR.sd.1 = aaply(laply(all.dat, as.matrix), c(2, 3), sd) # estandar deviation of three replicates
FLR.cv.1<-FLR.sd.1/FLR.1 # var coef of three replicates

count(as.data.frame(FLR.cv.1>0.3))

#There are some value in our plates that are very different from their replicates
#For example HAL C 40-42 ANA T0 (A1: 0.5, A5: 0.1, A9: 0.1)
#In this case we want to identify the deviated value and eliminate it
#From FLR.cv we are going to get a list of compounds and plates were cv is higher than 0.3

coords<-as.data.frame(which(FLR.cv.1 > 0.3, arr.ind = TRUE))

CV.1<- data.frame(R1=numeric(), 
                  R2=numeric(),
                  R3=numeric())#dataframe values to check

for(i in 1:nrow(coords)) {
  
  CV.1[i,1]<-FR[coords[i,1],coords[i,2]]
  CV.1[i,2]<-SR[coords[i,1],coords[i,2]]
  CV.1[i,3]<-TR[coords[i,1],coords[i,2]]
  
}

CV.1<-cbind(coords,CV.1) # visual identification of outlayer

write.csv(CV.1,"CV.1.csv",sep=";", dec=",")

### FT2 ####
FR<-FT2[,c(7:38)]#First replicate
SR<-FT2[,c(39:70)]#second replicate
TR<-FT2[,c(71:102)]#third replicate

all.dat <- list(dat1=FR, dat2=SR, dat3=TR)
FLR.2 = aaply(laply(all.dat, as.matrix), c(2, 3), mean) # media de las tres replicas
FLR.sd.2 = aaply(laply(all.dat, as.matrix), c(2, 3), sd) # estandar deviation of three replicates
FLR.cv.2<-FLR.sd.2/FLR.2 # var coef of three replicates

count(as.data.frame(FLR.cv.2>0.3))

#There are some value in our plates that are very different from their replicates
#For example HAL C 40-42 ANA T0 (A1: 0.5, A5: 0.1, A9: 0.1)
#In this case we want to identify the deviated value and eliminate it
#From FLR.cv we are going to get a list of compounds and plates were cv is higher than 0.3

coords<-as.data.frame(which(FLR.cv.2 > 0.3, arr.ind = TRUE))

CV.2<- data.frame(R1=numeric(), 
                  R2=numeric(),
                  R3=numeric())#dataframe values to check

for(i in 1:nrow(coords)) {
  
  CV.2[i,1]<-FR[coords[i,1],coords[i,2]]
  CV.2[i,2]<-SR[coords[i,1],coords[i,2]]
  CV.2[i,3]<-TR[coords[i,1],coords[i,2]]
  
}

CV.2<-cbind(coords,CV.2) # visual identification of outlayer

write.csv(CV.2,"CV.2.csv",sep=";", dec=",")

### FT3 ####
FR<-FT3[,c(7:38)]#First replicate
SR<-FT3[,c(39:70)]#second replicate
TR<-FT3[,c(71:102)]#third replicate

all.dat <- list(dat1=FR, dat2=SR, dat3=TR)
FLR.3 = aaply(laply(all.dat, as.matrix), c(2, 3), mean) # media de las tres replicas
FLR.sd.3 = aaply(laply(all.dat, as.matrix), c(2, 3), sd) # estandar deviation of three replicates
FLR.cv.3<-FLR.sd.3/FLR.3 # var coef of three replicates

count(as.data.frame(FLR.cv.3>0.3))

#There are some value in our plates that are very different from their replicates
#For example HAL C 40-42 ANA T0 (A1: 0.5, A5: 0.1, A9: 0.1)
#In this case we want to identify the deviated value and eliminate it
#From FLR.cv we are going to get a list of compounds and plates were cv is higher than 0.3

coords<-as.data.frame(which(FLR.cv.3 > 0.3, arr.ind = TRUE))

CV.3<- data.frame(R1=numeric(), 
                  R2=numeric(),
                  R3=numeric())#dataframe values to check

for(i in 1:nrow(coords)) {
  
  CV.3[i,1]<-FR[coords[i,1],coords[i,2]]
  CV.3[i,2]<-SR[coords[i,1],coords[i,2]]
  CV.3[i,3]<-TR[coords[i,1],coords[i,2]]
  
}

CV.3<-cbind(coords,CV.3) # visual identification of outlayer

write.csv(CV.3,"CV.3.csv",sep=";", dec=",")


