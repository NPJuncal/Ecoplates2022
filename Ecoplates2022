T2T.f.c[which(T2T.f.c$Station == "ATR" & T2T.f.c$Depth == 42 & T2T.f.c$Met == "AER"),]<-
  c(as.character(T2T.f.c[which(T2T.f.c$Station == "ATR" & T2T.f.c$Depth == 42 & T2T.f.c$Met == "AER"),c(1:6)]), as.numeric(colMeans(na.omit(ATR_Aer_42[,c(7:38)]))))

