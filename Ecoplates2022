# T2T ####
#data.frame with the replicate plates
Rep2<-subset(T2T.f,T2T.f$Depth == 4 | T2T.f$Depth == 42)

DFA<-subset(T2T.f, T2T.f$Station == "ATR" & T2T.f$Core == "C")
DFB<-subset(T2T.f, T2T.f$Station =="BOL" & T2T.f$Core == "A")
DFJ<-subset(T2T.f, T2T.f$Station =="J" & T2T.f$Core == "C")
DFBB<-subset(T2T.f, T2T.f$Station =="BB" & T2T.f$Core == "A")

T2T.f.c<-as.data.frame(rbind(DFA,DFB,DFJ,DFBB))

#substitute 4 and 42 plates by the average of the replicates

ATR_Ana_4<-subset(Rep2,Rep2$Station == "ATR" & Rep2$Depth == 4 & Rep2$Met == "ANA")
T2T.f.c[which(T2T.f.c$Station == "ATR" & T2T.f.c$Depth == 4 & T2T.f.c$Met == "ANA"),]<-
  c(as.character(T2T.f.c[which(T2T.f.c$Station == "ATR" & T2T.f.c$Depth == 4 & T2T.f.c$Met == "ANA"),c(1:6)]), as.numeric(colMeans(ATR_Ana_4[,c(7:38)])))
ATR_Aer_4<-subset(Rep2,Rep2$Station == "ATR" & Rep2$Depth == 4 & Rep2$Met == "AER")
T2T.f.c[which(T2T.f.c$Station == "ATR" & T2T.f.c$Depth == 4 & T2T.f.c$Met == "AER"),]<-
  c(as.character(T2T.f.c[which(T2T.f.c$Station == "ATR" & T2T.f.c$Depth == 4 & T2T.f.c$Met == "AER"),c(1:6)]), as.numeric(colMeans(ATR_Aer_4[,c(7:38)])))
ATR_Ana_42<-subset(Rep2,Rep2$Station == "ATR" & Rep2$Depth == 42 & Rep2$Met == "ANA")
T2T.f.c[which(T2T.f.c$Station == "ATR" & T2T.f.c$Depth == 42 & T2T.f.c$Met == "ANA"),]<-
  c(as.character(T2T.f.c[which(T2T.f.c$Station == "ATR" & T2T.f.c$Depth == 42 & T2T.f.c$Met == "ANA"),c(1:6)]), as.numeric(colMeans(ATR_Ana_42[,c(7:38)])))
